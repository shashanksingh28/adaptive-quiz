#content-container.container
  .row
    #left-col.col-md-7.hidden-xs
      #concept-tree.panel.panel-primary
        .panel-heading
          h2 Concepts
        .panel-body
          h4 Concepts>Content
          h4 Concepts>Content
          h4 Concepts>Content
          h4 Concepts>Content
          h4 Concepts>Content
